
#include <iostream>
using namespace std;

int main() {
	// star pattern 22
    int i,j,k;
    for(i=1;i<=6;i++){
        k=1;
        for(j=1;j<=6;j++){
            if(j<=i){
            cout<<k;
            k=1-k;}
            else{
            cout<<" ";}
        }    
        cout<<"\n";
   }
}
