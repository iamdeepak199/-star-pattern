
#include <iostream>
using namespace std;

int main() {
	// star pattern 9
    int i,j;
    char k;
    
  for(i=1;i<=4;i++)
  {       k='A';
      for(j=1;j<=7;j++)
      {
      if(j<=5-i||j>=3+i)
      {
      cout<<k;

      }
      else
      {
          cout<<" ";
      }
   j<4?k++:k--;
    }
     cout<<"\n";
}

         }
