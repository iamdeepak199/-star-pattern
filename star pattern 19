
#include <iostream>
using namespace std;

int main() {
	// star pattern 24
    int i,j;
    char x;
    for(i=1;i<=4;i++){

           x=64+i;
        for(j=1;j<=4;j++){
            if(i>=j){
            cout<<x;
             x--;}
            else{
            cout<<" ";}
        }
        cout<<"\n";
    }
}

